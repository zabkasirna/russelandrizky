{
    "name": "rdt/russelandrizky",
    "description": "project package for russel and rizky website",
    "license": "proprietary",
    "homepage": "http://russelandrizky.dev",
    "authors": [
        {
            "name": "sirna",
            "email": "zabka.sirna.gwen@gmail.com",
            "homepage": "http://sirna.id",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.2.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.2.2.zip"
                },
                "require": {
                    "fancyguy/webroot-installer": "1.1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "wordpress": "4.2.2",
        "fancyguy/webroot-installer": "1.1.0",
        "wpackagist-plugin/hide-admin-bar": "0.2.7"
    },
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 1000
    },
    "extra": {
        "webroot-dir": "site",
        "webroot-package": "wordpress",
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "require-dev": {
    }
}
